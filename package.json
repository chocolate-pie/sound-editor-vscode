{
  "name": "sound-editor",
  "displayName": "Sound Editor",
  "description": "Sound Editor for VSCode",
  "version": "0.0.1",
  "publisher": "chocolate-pie",
  "license": "BSD 3-Clause License",
  "engines": {
    "vscode": "^1.74.0"
  },
  "keywords": [
    "sound-editor",
    "editor",
    "sound",
    "audio"
  ],
  "categories": [
    "Other"
  ],
  "author": {
    "name": "chocolate-pie",
    "url": "https://github.com/chocolate-pie"
  },
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chocolate-pie/sound-editor-vscode.git"
  },
  "homepage": "https://github.com/chocolate-pie/sound-editor-vscode",
  "bugs": {
    "url": "https://github.com/chocolate-pie/sound-editor-vscode/issues/"
  },
  "activationEvents": [
    "onCustomEditor:SoundEditor.CoreSoundEditor"
  ],
  "resolutions": {
    "minimist": ">=0.2.4",
    "lodash.template": ">=4.5.0",
    "trim-newlines": ">=3.0.1",
    "glob-parent": ">=5.1.2"
  },
  "main": "./dist/extension.js",
  "browser": "./dist/web/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "SoundEditor.CoreSoundEditor",
        "displayName": "Sound Editor",
        "selector": [
          {
            "filenamePattern": "*.wav"
          },
          {
            "filenamePattern": "*.mp3"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write ./src",
    "format-all": "prettier --write **/*.{js,json,md}"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-raster": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "mocha": "^10.1.0",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.8.6",
    "process": "^0.11.10",
    "ts-loader": "^9.4.1",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "lamejs": "github:zhuker/lamejs",
    "path-browserify": "^1.0.1"
  }
}
